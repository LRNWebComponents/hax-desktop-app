{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "bash build.sh",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . hax-desktop-app --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Hax-Desktop app\"",
    "package-linux": "electron-packager . hax-desktop-app --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.4"
  },
  "dependencies": {
    "case": "^1.5.4",
    "cheerio": "^1.0.0-rc.2",
    "electron-packager": "^12.0.1",
    "electron-store": "^1.3.0",
    "markdown": "^0.5.0",
    "marked": "^0.3.7",
    "rxjs": "^5.5.5",
    "showdown": "^1.8.6",
    "turndown": "^4.0.1",
    "underscore": "^1.8.3"
  }
}
