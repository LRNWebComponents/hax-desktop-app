{
  "name": "hax-desktop-app",
  "version": "1.0.0",
  "description": "A desktop interface for using HAX.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "bash build.sh",
    "debug-demo": "cd app && polymer serve",
    "test": "mocha",
    "package-mac": "electron-packager . --overwrite --icon=assets/icons/mac/lrn_icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . hax-desktop-app --overwrite --asar --icon=assets/icons/win/lrn_icon.ico  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Hax-Desktop app\"",
    "package-linux": "electron-packager . hax-desktop-app --overwrite --asar --icon=assets/icons/lin/lrn_icon.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/LRNWebComponents/hax-desktop-app",
  "keywords": [
    "Electron",
    "HAX"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "mocha": "^5.1.1"
  },
  "dependencies": {
    "case": "^1.5.4",
    "cheerio": "^1.0.0-rc.2",
    "electron-packager": "^12.0.1",
    "electron-store": "^1.3.0",
    "electron-window-state": "^4.1.1",
    "filenamify": "^2.0.0",
    "gitbook-2-outline-schema": "^0.1.5",
    "graphql-yoga": "^1.13.1",
    "markdown": "^0.5.0",
    "marked": "^0.3.7",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.5",
    "showdown": "^1.8.6",
    "turndown": "^4.0.1",
    "underscore": "^1.8.3",
    "unique-file-name": "^0.1.7",
    "unused-filename": "^1.0.0",
    "uuid": "^3.2.1"
  }
}
